{
  "disabled": false,
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "menucard-{id}/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "myOutputBlob",
      "type": "blob",
      "path": "menucard-{id}/thumbnail.png",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
